boolean test_level_colide(float x,float y){
 // loaded_level=loadJSONArray("test.json");
         for(int i=0;i<loaded_level.size();i++){
          JSONObject block=loaded_level.getJSONObject(i); 
          String type=block.getString("type");
          
          float tx = block.getFloat("x"),ty = block.getFloat("y"),dx = block.getFloat("dx"),dy = block.getFloat("dy");
          float x2 = tx+dx,y2=ty+dy;
